'use strict';
exexSMSApp.controller('estimateCustomInfoCtrl',function($rootScope, $scope, $uibModalInstance){

    $rootScope.data = {
            customInfo: '',
    }
    //-- Methods --//
    $scope.done = function(){
        $uibModalInstance.close($rootScope.data.customInfo);
    }; // end done
    $scope.no = function(){
        $uibModalInstance.dismiss('no');
    }; // end close
});